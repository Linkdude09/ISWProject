            <Button fx:id="atkBTN" layoutX="53.0" layoutY="316.0" mnemonicParsing="false" text="Attack" />
            <Button fx:id="itemBTN" layoutX="106.0" layoutY="316.0" mnemonicParsing="false" text="Use Item" />
            <TextArea fx:id="PlayField" layoutX="23.0" layoutY="26.0" prefHeight="214.0" prefWidth="595.0" />
            <TextArea fx:id="StatusTextFeild" layoutX="342.0" layoutY="285.0" prefHeight="87.0" prefWidth="228.0" />
